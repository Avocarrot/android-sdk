<com.avocarrot.sdk.nativead.NativeAdView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    app:avoStarRatingStyle="MEDIUM"
    app:avoTitleTypeface="MONOSPACE"
    tools:ignore="UseCompoundDrawables,ContentDescription">

    <android.support.constraint.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <TextView
            android:id="@+id/native_ad_attribution"
            android:layout_width="68dp"
            android:layout_height="17dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:gravity="center"
            android:lines="1"
            android:textIsSelectable="false"
            app:layout_constraintRight_toLeftOf="@+id/native_ad_choices_container"
            app:layout_constraintTop_toTopOf="parent"
            tools:layout_constraintRight_creator="1"
            tools:layout_constraintTop_creator="1"
            tools:text="Sponsored"
            tools:textColor="@android:color/darker_gray"
            android:layout_marginEnd="8dp" />

        <ImageView
            android:id="@+id/native_ad_icon"
            android:layout_width="64dp"
            android:layout_height="64dp"
            android:layout_marginLeft="8dp"
            android:layout_marginTop="9dp"
            android:background="@null"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/native_ad_media_layout"
            tools:layout_conversion_absoluteHeight="64dp"
            tools:layout_conversion_absoluteWidth="64dp"
            tools:layout_conversion_absoluteX="16dp"
            tools:layout_conversion_absoluteY="476dp"
            tools:src="@android:drawable/safe_mode_background"
            android:layout_marginStart="8dp" />


        <TextView
            android:id="@+id/native_ad_title"
            android:layout_width="0dp"
            android:layout_height="16dp"
            android:layout_marginEnd="8dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:ellipsize="end"
            android:lines="1"
            android:textIsSelectable="false"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/native_ad_icon"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/native_ad_media_layout"
            tools:layout_conversion_absoluteHeight="16dp"
            tools:layout_conversion_absoluteWidth="368dp"
            tools:layout_conversion_absoluteX="96dp"
            tools:layout_conversion_absoluteY="238dp"
            tools:text="Ad title. Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum."
            tools:textColor="@android:color/darker_gray"
            tools:textStyle="bold" />

        <RatingBar
            android:id="@+id/native_ad_star_rating"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="8dp"
            app:layout_constraintLeft_toRightOf="@+id/native_ad_icon"
            app:layout_constraintTop_toBottomOf="@+id/native_ad_title"
            tools:layout_conversion_absoluteHeight="57dp"
            tools:layout_conversion_absoluteWidth="240dp"
            tools:layout_conversion_absoluteX="96dp"
            tools:layout_conversion_absoluteY="254dp"
            android:layout_marginStart="8dp" />


        <TextView
            android:id="@+id/native_ad_text"
            android:layout_width="200dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:ellipsize="end"
            android:maxLines="2"
            android:paddingEnd="10dp"
            android:paddingLeft="10dp"
            android:paddingRight="10dp"
            android:paddingStart="10dp"
            android:textIsSelectable="false"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/native_ad_icon"
            tools:layout_constraintLeft_creator="1"
            tools:text="Brief description of the Ad goes here. Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum."
            tools:textColor="@android:color/darker_gray"
            tools:visibility="visible" />

        <FrameLayout
            android:id="@+id/native_ad_choices_container"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:minHeight="20dp"
            android:minWidth="20dp"
            android:paddingEnd="0dp"
            android:paddingLeft="8dp"
            android:paddingRight="0dp"
            android:paddingStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/native_ad_media_layout"
            app:layout_constraintHorizontal_bias="0.977"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" />

        <ImageView
            android:id="@+id/native_ad_choices_icon"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="8dp"
            android:minHeight="20dp"
            android:minWidth="20dp"
            android:paddingEnd="0dp"
            android:paddingLeft="8dp"
            android:paddingRight="0dp"
            android:paddingStart="8dp"
            app:layout_constraintBottom_toTopOf="@+id/native_ad_media_layout"
            app:layout_constraintHorizontal_bias="0.977"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp" />


        <FrameLayout
            android:id="@+id/native_ad_media_layout"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintRight_toRightOf="parent"
            tools:layout_constraintBottom_creator="1"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintRight_creator="1"
            tools:visibility="visible"
            android:layout_marginTop="8dp"
            app:layout_constraintTop_toBottomOf="@+id/native_ad_attribution">

            <FrameLayout
                android:id="@+id/native_ad_media_container"
                android:layout_width="0dp"
                android:layout_height="218dp"
                android:gravity="center"
                android:visibility="gone" />

            <ImageView
                android:id="@+id/native_ad_image"
                android:layout_width="match_parent"
                android:layout_height="218dp"
                android:background="@null"
                android:maxHeight="218dp"
                android:scaleType="centerCrop"
                android:visibility="gone"
                tools:src="@android:drawable/safe_mode_background"
                tools:visibility="visible" />

        </FrameLayout>

        <Button
            android:id="@+id/native_ad_call_to_action"
            style="@style/avo_nativead_lower_case.avo_nativead_feed_call_to_action_button"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_marginBottom="0dp"
            android:layout_marginLeft="8dp"
            android:layout_marginRight="8dp"
            android:layout_marginTop="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintLeft_toRightOf="@+id/native_ad_text"
            app:layout_constraintRight_toRightOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/native_ad_icon"
            app:layout_constraintVertical_bias="0.0"
            tools:layout_constraintLeft_creator="1"
            tools:layout_constraintTop_creator="1"
            tools:text="Install. Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo consequat. Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur. Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia deserunt mollit anim id est laborum." />
    </android.support.constraint.ConstraintLayout>

</com.avocarrot.sdk.nativead.NativeAdView>